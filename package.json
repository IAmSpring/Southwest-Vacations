{
  "name": "southwest-vacations-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"FRONTEND,BACKEND\" -c \"blue,green\" \"vite\" \"npx cross-env TS_NODE_PROJECT=mock-backend/tsconfig.json node --loader ts-node/esm mock-backend/index.ts\"",
    "dev:frontend": "vite",
    "dev:backend": "npx cross-env TS_NODE_PROJECT=mock-backend/tsconfig.json node --loader ts-node/esm mock-backend/index.ts",
    "dev:all": "concurrently -n \"FRONTEND,BACKEND,TEST-VIZ\" -c \"blue,green,yellow\" \"vite\" \"npx cross-env TS_NODE_PROJECT=mock-backend/tsconfig.json node --loader ts-node/esm mock-backend/index.ts\" \"npm run test:viz\"",
    "start": "vite preview",
    "clean-start": "./scripts/clean-startup.sh",
    "dev:ai": "node ai-test-backend.js",
    "copy-docs": "node scripts/copy-docs.js",
    "prebuild": "npm run copy-docs",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:booking": "node cypress/run-booking-tests.js",
    "test:features": "node --loader ts-node/esm src/test-startup.js",
    "test:pw": "npx playwright test",
    "test:pw:ui": "npx playwright test --ui",
    "test:pw:login": "npx playwright test tests/login.spec.ts",
    "test:pw:features": "npx playwright test tests/login.spec.ts tests/multi-passenger-booking.spec.ts tests/training-certification.spec.ts tests/multi-location-booking.spec.ts tests/search-filters.spec.ts tests/ai-assistant.spec.ts",
    "test:pw:ai-assistant": "npx playwright test tests/ai-assistant.spec.ts",
    "test:viz": "cd test-server && node start.js",
    "test:cypress:viz": "concurrently -n \"VIZ,CYPRESS\" -c \"yellow,blue\" \"npm run test:viz\" \"npm run cypress\"",
    "test:pw:viz": "concurrently -n \"VIZ,PLAYWRIGHT\" -c \"yellow,purple\" \"npm run test:viz\" \"npm run test:pw:ui\"",
    "fix:backend": "node --loader ts-node/esm scripts/fix-backend.js",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "prepare": "husky install",
    "system-test": "node scripts/test-system.js",
    "startup-tests": "node --loader ts-node/esm scripts/startup-tests.js",
    "create-test-users": "node scripts/create-test-users.js",
    "create-test-scenarios": "node scripts/create-test-scenarios.js",
    "download-images": "node scripts/download-images.js",
    "integration-test": "node scripts/integration-test.js",
    "test-admin-login": "node scripts/test-admin-login.js",
    "test-ai-assistant": "node --loader ts-node/esm scripts/test-ai-assistant.js",
    "playwright:install": "npx playwright install",
    "playwright:test": "npx playwright test",
    "playwright:ui": "npx playwright test --ui",
    "test:login": "npx playwright test tests/login.spec.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@types/morgan": "^1.9.9",
    "@types/react-helmet": "^6.1.11",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lowdb": "^1.0.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^9.1.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.23.0",
    "recharts": "^2.15.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lowdb": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.0.0",
    "abort-controller": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^1.3.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.0",
    "vite": "^4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  }
}
