{
  "name": "southwest-vacations-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"FRONTEND,BACKEND\" -c \"blue,green\" \"vite\" \"npx cross-env TS_NODE_PROJECT=mock-backend/tsconfig.json node --loader ts-node/esm mock-backend/index.ts\"",
    "dev:frontend": "vite",
    "dev:backend": "npx cross-env TS_NODE_PROJECT=mock-backend/tsconfig.json node --loader ts-node/esm mock-backend/index.ts",
    "start": "vite preview",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "prepare": "husky install",
    "system-test": "node scripts/test-system.js",
    "create-test-users": "node scripts/create-test-users.js",
    "create-test-scenarios": "node scripts/create-test-scenarios.js",
    "download-images": "node scripts/download-images.js",
    "integration-test": "node scripts/integration-test.js",
    "test-admin-login": "node scripts/test-admin-login.js"
  },
  "dependencies": {
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lowdb": "^3.0.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.23.0",
    "uuid": "^11.1.0",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lowdb": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.0.0",
    "abort-controller": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^1.3.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.0",
    "vite": "^4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  }
}
